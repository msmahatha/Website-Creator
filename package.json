  },
  "homepage": "https://github.com/yourusername/website-inspector-pro#readme",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "cors": "^2.8.5",
